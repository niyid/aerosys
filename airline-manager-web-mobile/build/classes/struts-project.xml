	<package name="upload" namespace="/upload" extends="operator">
		<interceptors>
			<interceptor-stack name="photoUploadStack">
				<interceptor-ref name="exception" />
				<interceptor-ref name="alias" />
				<interceptor-ref name="servletConfig" />
				<interceptor-ref name="prepare" />
				<interceptor-ref name="i18n" />
				<interceptor-ref name="chain" />
				<interceptor-ref name="debugging" />
				<interceptor-ref name="profiling" />
				<interceptor-ref name="scopedModelDriven" />
				<interceptor-ref name="modelDriven" />
				<interceptor-ref name="fileUpload">
					<param name="maximumSize">102400</param>
					<param name="allowedTypes">image/jpeg, image/pjpeg</param>
				</interceptor-ref>
				<interceptor-ref name="checkbox" />
				<interceptor-ref name="staticParams" />
				<interceptor-ref name="actionMappingParams" />
				<interceptor-ref name="params">
					<param name="excludeParams">dojo\..*,^struts\..*</param>
				</interceptor-ref>
				<interceptor-ref name="conversionError" />
				<interceptor-ref name="validation">
					<param name="excludeMethods">input,back,cancel,browse</param>
				</interceptor-ref>
				<interceptor-ref name="workflow">
					<param name="excludeMethods">input,back,cancel,browse</param>
				</interceptor-ref>
			</interceptor-stack>
		</interceptors>
	    	    	    
	    <action name="photoUpload" class="photoUploadAction">
	        <interceptor-ref name="photoUploadStack" />
			<result name="input">/WEB-INF/jsp/passenger/ajax/photoUpload.jsp</result>
			<result name="success">/WEB-INF/jsp/passenger/ajax/photoUpload.jsp</result>
			<result name="error">/WEB-INF/jsp/passenger/ajax/photoUpload.jsp</result>
	    </action>
	    	    	    
	    <action name="itemPhotoUpload" class="itemPhotoUploadAction">
	        <interceptor-ref name="photoUploadStack" />
			<result name="input">/WEB-INF/jsp/passenger/ajax/itemPhotoUpload.jsp</result>
			<result name="success">/WEB-INF/jsp/passenger/ajax/itemPhotoUpload.jsp</result>
			<result name="error">/WEB-INF/jsp/passenger/ajax/itemPhotoUpload.jsp</result>
	    </action>    	    
	</package>
		
